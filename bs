local lp = game.Players.LocalPlayer

--// bl check
local blacklist = {"CahyaXyZp", "bludisdancer"}
for _, n in ipairs(blacklist) do
    if lp.Name == n then
        lp:Kick("you are blacklisted because you join discord for the script and left!")
        break
    end
end

--// exe detection & webhook
local http = game:GetService("HttpService")
local exec = "Unknown"
if syn then exec = "Synapse X"
elseif secure_load then exec = "Sentinel"
elseif pebc_execute then exec = "KRNL"
elseif identifyexecutor then exec = identifyexecutor()
elseif hookfunction and checkcaller and setreadonly then exec = "Delta" end

local gName = "Unknown"
pcall(function()
    gName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
end)

local data = {
    content = string.format("7 gays\nUsername: %s\nDisplay Name: %s\nUser ID: %s\nExecutor: %s\nPlace ID: %s\nPlace Name: %s",
        lp.Name, lp.DisplayName, lp.UserId, exec, game.PlaceId, gName)
}

local js = http:JSONEncode(data)
local req = syn and syn.request or request or http and http.request
if req then
    req({
        Url = "https://discord.com/api/webhooks/1364038040615387197/tM9OvHCXAslpJdINWSSa-wATcLwh7hKMTWL0g9JwW7nSFEi66NwOeSA3x2R7r1-P5IEV",
        Method = "POST",
        Headers = {["Content-Type"] = "application/json"},
        Body = js
    })
end


local L = loadstring(game:HttpGet("https://raw.githubusercontent.com/0-BaconScripter-0/Mobile-Support-For-UI-Library/refs/heads/main/Unnamed.lua"))()
local w = L:CreateWindow("7 days to live")
local tTab = w:AddFolder("toggles")
local bTab = w:AddFolder("buttons")


tTab:AddToggle({
    text = "aura kill",
    state = false,
    callback = function(state)
        _G.ak = state
        if state then
            local radius = 100
            task.spawn(function()
                while _G.ak do
                    local char = lp.Character
                    local hrp = char and char:FindFirstChild("HumanoidRootPart")
                    if hrp then
                        for _, scp in ipairs(workspace.scps:GetChildren()) do
                            local target = scp:FindFirstChild("HumanoidRootPart") or scp:FindFirstChild("Head")
                            local hum = scp:FindFirstChildWhichIsA("Humanoid")
                            if target and hum and hum.Health > 0 then
                                local dist = (target.Position - hrp.Position).Magnitude
                                if dist < radius then
                                    local from = target.CFrame + Vector3.new(0, 0.5, 0)
                                    local to = target.CFrame
                                    game.ReplicatedStorage.remotes.shoot:FireServer(from, to)
                                end
                            end
                        end
                    end
                    task.wait(0.25)
                end
            end)
        end
    end
})


tTab:AddToggle({
    text = "instant chop tree(near tree)",
    state = false,
    callback = function(v)
        _G.ct = v
        if v then
            task.spawn(function()
                while _G.ct do
                    game.ReplicatedStorage.remotes.swing_axe:FireServer()
                    task.wait(0.01)
                end
            end)
        end
    end
})

tTab:AddToggle({
    text = "auto farm scrap",
    state = false,
    callback = function(v)
        _G.af = v
        if v then
            task.spawn(function()
                local hrp = lp.Character:WaitForChild("HumanoidRootPart")
                while _G.af do
                    for _, s in pairs(workspace.scraps:GetChildren()) do
                        if not _G.af then break end
                        local bp = s:FindFirstChildWhichIsA("BasePart")
                        if bp then
                            hrp.CFrame = bp.CFrame + Vector3.new(0, 2, 0)
                            task.wait(0.1)
                            for _, o in pairs(workspace:GetDescendants()) do
                                if o:IsA("ProximityPrompt") and o.Parent:IsA("BasePart") and (hrp.Position - o.Parent.Position).Magnitude < 15 then
                                    fireproximityprompt(o)
                                end
                            end
                        end
                    end
                    task.wait()
                end
            end)
        end
    end
})

tTab:AddToggle({
    text = "fast active proximity prompts",
    state = false,
    callback = function(v)
        _G.app = v
        if v then
            task.spawn(function()
                local hrp = lp.Character:WaitForChild("HumanoidRootPart")
                while _G.app do
                    for _, o in pairs(workspace:GetDescendants()) do
                        if o:IsA("ProximityPrompt") and o.Parent:IsA("BasePart") and (hrp.Position - o.Parent.Position).Magnitude < 15 then
                            fireproximityprompt(o)
                        end
                    end
                    task.wait()
                end
            end)
        end
    end
})

tTab:AddToggle({
    text = "active proximity prompts",
    state = false,
    callback = function(v)
        _G.fapp = v
        if v then
            task.spawn(function()
                local c = lp.Character or lp.CharacterAdded:Wait()
                local hrp = c:WaitForChild("HumanoidRootPart")
                local r = 15
                while _G.fapp do
                    for _, o in pairs(workspace:GetDescendants()) do
                        if not _G.fapp then break end
                        if o:IsA("ProximityPrompt") and o.Parent:IsA("BasePart") then
                            if (hrp.Position - o.Parent.Position).Magnitude <= r then
                                fireproximityprompt(o)
                            end
                        end
                    end
                    task.wait(0.1)
                end
            end)
        end
    end
})

bTab:AddButton({
    text = "teleport to spawn",
    callback = function()
        local hrp = lp.Character:WaitForChild("HumanoidRootPart")
        hrp.CFrame = CFrame.new(57, 42, 416)
    end
})

bTab:AddButton({
    text = "instant max hunger(any food)",
    callback = function()
        for i = 1, 20 do
            pcall(function() lp.Character.Cabbage.eat:FireServer() end)
            pcall(function() lp.Character.Beans.eat:FireServer() end)
            pcall(function() lp.Character.Berry.eat:FireServer() end)
        end
    end
})

bTab:AddLabel({
    text = "Made by Pall(@opalbetus)"
})

L:Init()